<Window x:Class="AsciiImage.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AsciiImage"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="file" Width="50" Height="40" Margin="329,0,0,394" VerticalAlignment="Bottom" Click="file_Click" HorizontalAlignment="Left" />

        <Button x:Name="run" Width=" 50" Height="40 " Margin="410,0,0,394" VerticalAlignment="Bottom" HorizontalAlignment="Left" Click="run_Click" />
        <TextBlock x:Name="content" Height="370" VerticalAlignment="Bottom" HorizontalAlignment="Center" Width="800"/>

    </Grid>
</Window>
